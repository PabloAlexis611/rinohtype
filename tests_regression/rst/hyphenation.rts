[STYLESHEET]
base = sphinx_base14


[spacer : Paragraph(has_class='spacer')]
border = 1pt, #555
padding_top = 6cm
padding_bottom = 6cm
text_align = center

[body]
margin_left = 1cm
margin_right = 1cm
hyphenate = true
hyphen_chars = 2

[narrow : Paragraph(has_class='narrow')]
border = 1pt, #555
space_above = 1cm
margin_right = 7cm
padding = 2pt
